# Aliases

# Enables dircolors for files
alias ls='gls --color=auto'
alias ll='ls -al'
alias la='ls -al'

# Directory navigation
alias ..='cd ..'
alias cd..='cd ..' #typos

# Git
alias g='git'
# I commit this mistake to often.
alias gi='git'
# I do `git status` as much as `ls` and that's a lot ðŸ˜….
alias gs='git status'
# Decortate git with hub to have pr, fork, issue...
eval "$(hub alias -s)"

alias beep='echo -e "\a"'
alias done='say -v Victoria done'

alias reload='source ~/.zsh/'
